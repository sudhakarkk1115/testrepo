pipeline {
    agent any
    stages {
        stage("Welcome to Jenkins"){
            steps {
                script {
                    a=10
                    b=20
                    if(a > b) {
                        println "${a} is big"
                    }
                    else {
                        println "${b} is big"
                    }

                    
                }
            }

        }
    }
}
